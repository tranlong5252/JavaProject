package baitap;

import java.util.Scanner;

public class solonnhat {
	public static void main(String[] args) {
		System.out.println("Moi nhap so thu nhat");
		
		int sothunhat = scanner.nextInt();
		System.out.println("Moi nhap so thu hai");
		int sothuhai = scanner.nextInt();
		System.out.println("Moi nhap so thu ba");
		int sothuba = scanner.nextInt();
		
		int max = 0;
		
		if(sothunhat>sothuhai && sothunhat>sothuba) {
			max=sothunhat;
		}else if(sothuhai>sothunhat && sothuhai>sothuba) {
			max=sothuhai;
		}else {
			max=sothuba;
		}
		
		System.out.println("so lon nhat giua " + sothunhat + ", " + sothuhai + ", " + sothuba + " la " + max);
	}
}
